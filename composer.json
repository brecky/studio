{
    "name": "orchestra/studio",
    "description": "Code Generator for Orchestra Platform",
    "keyword": ["orchestra-platform", "orchestral", "code-generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Studio\\" : "src/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "orchestra/support": "~3.3.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "illuminate/console": "~5.3.0",
        "illuminate/filesystem": "~5.3.0",
        "league/flysystem": "~1.0"
    },
    "suggest": {
        "wikimedia/composer-merge-plugin": "Add workbench support for Orchestra Platform (~1.1)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    },
    "minimum-stability": "dev"
}
